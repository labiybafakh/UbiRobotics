extern "C"
{
 
  void ModuleNameTestInit(RTC::Manager* manager)
  {
    coil::Properties profile(modulename_spec);
    manager->registerFactory(profile,
                             RTC::Create<ModuleNameTest>,
                             RTC::Delete<ModuleNameTest>);
  }
  
};


